<Resource>
  <Instance name="i1">
    <Instance ref.Class="MainWindow">
      <ref.Parameter Name="textBox" Class="SinglelineTextBox"/>
      <ref.Parameter Name="self" Class="presentation::controls::GuiSinglelineTextBox"/>
      <Window ref.Name="self" Border="Not A Bool" TooltipControl="Not Serializable">
        <att.BoundsComposition-set AlignmentToParent="Not A Margin"/>
        <Button ref.Name="self"/>

        <ComboBox>
          <att.ListControl-bind>Constructor Argument Not Bindable</att.ListControl-bind>
        </ComboBox>
        <TreeView>
          <att.Nodes>
            <TreeNode Text-bind="Constructor Argument Not bind-Bindable"/>
          </att.Nodes>
        </TreeView>
        <BindableDataGrid>
          <att.Text-NotExists>Binder Not Exists</att.Text-NotExists>
          <att.Font-bind>
            <Button Text="Not A String"/>
          </att.Font-bind>
        </BindableDataGrid>

        <Table ev.BoundsChanged-NotExists="Binder Not Exists">
          <att.Columns>
            <CellOption ref.Name="notAReferenceType"/>
            <CellOption>Wrong!</CellOption>
          </att.Columns>
        </Table>

        <ComboBox Text="Missing Constructor Argument"/>

        <String>Not A Control Or Composition</String>

        <Table>
          <att.Columns>
            <_ ref.Name="notAReferenceType"/>
            <_>Wrong!</_>
          </att.Columns>
        </Table>
      </Window>
    </Instance>
  </Instance>
</Resource>